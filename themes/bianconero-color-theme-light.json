      "settings": {
        "foreground": "#626969",
        "fontStyle": ""
      }
    },
    {
      "name": "At-Rule Header",
      "scope": "meta.at-rule.media.header.css",
      "settings": {
        "foreground": "#5f6f81"
      }
    },
    {
      "name": "CSS important keyword",
      "scope": [
