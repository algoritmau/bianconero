    {
      "name": "CSS Variables",
      "scope": ["variable.css", "variable.scss"],
      "settings": {
        "foreground": "#c7a688", // 75b36d
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Functions",
      "scope": [
        "support.function.misc.css",
        "support.function.misc.scss",
        "entity.name.function",
        "support.function.calc.css",
        "support.function.url.css"
      ],
      "settings": {
        "foreground": "#f1a7bd",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Functions Punctuation",
      "scope": [
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css",
        "punctuation.section.function.scss",
        // FIXME: Target opening parenthesis of sass functions
        "entity.name.function.close"
      ],
      "settings": {
        "foreground": "#f1a7bd",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS At-Rules",
      "scope": "keyword.control.at-rule",
      "settings": {
        "foreground": "#5f6f81",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Logical Operator and (media)",
